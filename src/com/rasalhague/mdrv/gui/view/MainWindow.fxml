            layoutX = "0.0"
            maxHeight = "-Infinity"
            maxWidth = "-Infinity"
            minHeight = "300.0"
            minWidth = "600.0"
            prefHeight = "556.4999000000025"
            prefWidth = "1057.0"
            xmlns:fx = "http://javafx.com/fxml/1"
            xmlns = "http://javafx.com/javafx/2.2"
            fx:controller = "com.rasalhague.mdrv.gui.MainWindowController">
    <padding>
        <Insets bottom = "5.0" left = "5.0" right = "5.0" top = "5.0" fx:id = "x1"/>
    </padding>
    <GridPane hgap = "0.0"
              vgap = "5.0"
              visible = "true"
              AnchorPane.bottomAnchor = "0.0"
              AnchorPane.leftAnchor = "0.0"
              AnchorPane.rightAnchor = "0.0"
              AnchorPane.topAnchor = "0.0">
        <children>
            <LineChart fx:id = "lineChart"
                       alternativeColumnFillVisible = "false"
                       alternativeRowFillVisible = "false"
                       animated = "true"
                       createSymbols = "true"
                       depthTest = "INHERIT"
                       disable = "false"
                       focusTraversable = "false"
                       horizontalGridLinesVisible = "true"
                       horizontalZeroLineVisible = "true"
                       legendSide = "BOTTOM"
                       legendVisible = "false"
                       maxHeight = "1.7976931348623157E308"
                       maxWidth = "1.7976931348623157E308"
                       minHeight = "-Infinity"
                       minWidth = "-Infinity"
                       mouseTransparent = "false"
                       prefHeight = "-1.0"
                       prefWidth = "-1.0"
                       style = ""
                       verticalGridLinesVisible = "true"
                       verticalZeroLineVisible = "true"
                       GridPane.columnIndex = "0"
                       GridPane.halignment = "CENTER"
                       GridPane.hgrow = "ALWAYS"
                       GridPane.rowIndex = "0"
                       GridPane.valignment = "CENTER"
                       GridPane.vgrow = "ALWAYS">
                <xAxis>
                    <NumberAxis animated = "false"
                                autoRanging = "false"
                                disable = "false"
                                focusTraversable = "false"
                                forceZeroInRange = "false"
                                label = "Frequency, MHz"
                                lowerBound = "2395.0"
                                minorTickVisible = "false"
                                side = "BOTTOM"
                                tickLabelRotation = "0.0"
                                tickLabelsVisible = "true"
                                tickLength = "5.0"
                                tickMarkVisible = "true"
                                tickUnit = "5.0"
                                upperBound = "2500.0"/>
                </xAxis>
                <yAxis>
                    <NumberAxis animated = "false"
                                autoRanging = "false"
                                disable = "false"
                                forceZeroInRange = "false"
                                label = "RSSI, dBm"
                                lowerBound = "-120.0"
                                minorTickCount = "5"
                                minorTickVisible = "false"
                                side = "LEFT"
                                tickLabelGap = "3.0"
                                tickLabelsVisible = "true"
                                tickLength = "5.0"
                                tickMarkVisible = "true"
                                tickUnit = "5.0"
                                upperBound = "-40.0"/>
                </yAxis>
                <padding>
                    <Insets fx:id = "x2"/>
                </padding>
            </LineChart>
            <Slider fx:id = "replaySlider"
                    blockIncrement = "10.0"
                    focusTraversable = "true"
                    majorTickUnit = "1.0"
                    max = "1.0"
                    minorTickCount = "0"
                    showTickLabels = "false"
                    showTickMarks = "false"
                    snapToTicks = "true"
                    value = "0.0"
                    visible = "true"
                    GridPane.columnIndex = "0"
                    GridPane.halignment = "CENTER"
                    GridPane.hgrow = "ALWAYS"
                    GridPane.rowIndex = "1"
                    GridPane.valignment = "CENTER"
                    GridPane.vgrow = "SOMETIMES"/>
