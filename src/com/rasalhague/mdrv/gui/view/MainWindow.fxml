                      disable = "false"
                      editable = "false"
                      maxHeight = "0.0"
                      maxWidth = "-1.0"
                      minHeight = "0.0"
                      minWidth = "0.0"
                      prefHeight = "130.0"
                      prefWidth = "-1.0"
                      text = ""
                      visible = "true"
                      wrapText = "true"
                      GridPane.columnIndex = "0"
                      GridPane.hgrow = "NEVER"
                      GridPane.rowIndex = "2"/>
        </children>
        <columnConstraints>
            <ColumnConstraints halignment = "CENTER" hgrow = "ALWAYS" minWidth = "10.0" prefWidth = "-1.0"/>
        </columnConstraints>
        <padding>
            <Insets fx:id = "x2"/>
        </padding>
        <rowConstraints>
            <RowConstraints minHeight = "10.0" prefHeight = "-1.0" valignment = "CENTER" vgrow = "ALWAYS"/>
            <RowConstraints fillHeight = "true"
                            minHeight = "0.0"
                            prefHeight = "-1.0"
                            valignment = "CENTER"
                            vgrow = "NEVER"/>
            <RowConstraints minHeight = "0.0" valignment = "CENTER" vgrow = "NEVER"/>
        </rowConstraints>
    </GridPane>
    <GridPane fx:id = "tooltipPane"
              alignment = "CENTER"
              gridLinesVisible = "false"
              layoutX = "496.0"
              layoutY = "271.0"
              mouseTransparent = "true"
              visible = "true">
        <columnConstraints>
            <ColumnConstraints fillWidth = "true" hgrow = "SOMETIMES" minWidth = "-1.0" prefWidth = "-1.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight = "10.0" prefHeight = "-1.0" valignment = "CENTER" vgrow = "SOMETIMES"/>
        </rowConstraints>
    </GridPane>
    <TitledPane fx:id = "chartLegendPane"
                animated = "true"
                collapsible = "false"
                disable = "false"
                expanded = "true"
                minHeight = "-1.0"
                minWidth = "-1.0"
                mnemonicParsing = "false"
                mouseTransparent = "false"
                opacity = "1.0"
                pickOnBounds = "false"
                prefHeight = "-1.0"
                prefWidth = "-1.0"
                text = ""
                textOverrun = "ELLIPSIS"
                visible = "false"
                AnchorPane.rightAnchor = "0.0"
                AnchorPane.topAnchor = "0.0">
        <content>
            <VBox prefHeight = "-1.0" prefWidth = "-1.0"/>
        </content>
    </TitledPane>
    <VBox fx:id = "chartLegendVbox"
          mouseTransparent = "false"
          padding = "$x1"
          prefHeight = "-1.0"
          prefWidth = "-1.0"
          spacing = "3.0"
          AnchorPane.rightAnchor = "0.0"
          AnchorPane.topAnchor = "0.0"/>
    <Group id = "Group" layoutX = "0.0" layoutY = "0.0">
        <children>
            <Line fx:id = "horizontalLine"
                  endX = "100.0"
                  endY = "0.0"
                  layoutX = "0.0"
                  layoutY = "100.0"
                  mouseTransparent = "true"
                  scaleY = "1.0"
                  startX = "0.0"
                  translateY = "0.0"
                  visible = "false"/>
            <Line fx:id = "verticalLine"
                  cache = "false"
                  endX = "0.0"
                  endY = "100.0"
                  fill = "BLACK"
                  focusTraversable = "false"
                  layoutX = "500.0"
                  layoutY = "0.0"
                  mouseTransparent = "true"
                  opacity = "1.0"
                  smooth = "true"
                  startX = "0.0"
                  startY = "0.0"
                  strokeDashOffset = "1.0"
                  strokeLineCap = "ROUND"
                  strokeLineJoin = "MITER"
                  strokeMiterLimit = "1.0"
                  strokeType = "CENTERED"
                  strokeWidth = "0.8"/>
        </children>
    </Group>
    <VBox fx:id = "controlBntsVBox"
          fillWidth = "true"
          padding = "$x1"
          prefHeight = "-1.0"
          prefWidth = "-1.0"
          spacing = "3.0"
          visible = "true"
          AnchorPane.leftAnchor = "0.0"
          AnchorPane.topAnchor = "0.0">
        <children>
            <Button fx:id = "openReplayBtn"
                    mnemonicParsing = "false"
                    onAction = "#openReplayBtnClick"
                    text = "Open replay"
                    VBox.vgrow = "NEVER"/>
            <Button fx:id = "refreshChartButton"
                    cancelButton = "false"
                    defaultButton = "false"
                    mnemonicParsing = "false"
                    onMouseClicked = "#refreshChartButtonClickEvent"
                    text = "Refresh chart"
                    VBox.vgrow = "NEVER"/>
            <Button fx:id = "showDebugInfoBnt"
                    mnemonicParsing = "false"
                    onMouseClicked = "#showDebugInfoBntClick"
                    text = "Show debug info"
                    VBox.vgrow = "NEVER"/>
            <CheckBox fx:id = "maxCheckBox"
                      allowIndeterminate = "false"
                      indeterminate = "false"
                      mnemonicParsing = "false"
                      onMouseClicked = "#maxCheckBoxChangedEvent"
                      selected = "true"
                      text = "max"
                      underline = "false"/>
            <CheckBox id = "verticalLineSwitcher"
                      fx:id = "horizontalLineSwitcher"
                      mnemonicParsing = "false"
                      onAction = "#horizontalLineSwitchEvent"
                      selected = "false"
                      text = "Horizontal line"/>
            <CheckBox fx:id = "verticalLineSwitcher"
                      mnemonicParsing = "false"
                      onAction = "#verticalLineSwitchEvent"
                      selected = "true"
                      text = "Vertical line"/>
            <CheckBox fx:id = "replayModeSwitcher"
                      mnemonicParsing = "false"
                      onAction = "#replayModeSwitcherEvent"
                      text = "Manual replay mode"/>
        </children>
    </VBox>
</AnchorPane>
